@model KP8GlobalClient.Models.LoginModel
<br />
@*User : @Model.FName '|' Branch : @Model.Branch '|' Role : @Model.Role '|' Server : @Model.Server ──*@
<script>
    $(
        function () {
            ajaxLoadModal("Signed in");
            $.ajax({
                async: true,
                type: 'GET',
                url: '/Home/_headerLinks',
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                dataType: 'html',
                cache: false,
                success: function (result) {
                    ajaxLoadModal("Rendering tools");
                    $('nav#hdrLinks').html(result);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    unExpectedError();
                }
            });
        }
    )
</script>